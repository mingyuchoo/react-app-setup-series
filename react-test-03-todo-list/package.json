  "scripts": {
    "eject": "react-scripts eject",
    "lint": "eslint --config .eslintrc.yaml --fix './**/*.{ts,tsx}'",
    "sonar": "node sonar-project.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "run-s lint test:unit:coverage",
