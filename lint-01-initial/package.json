{
  "name": "lint-01-initial",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --write \"**/*.{js,json,md,scss,yaml,yml}\"",
    "lint": "eslint --config .eslintrc.yml --fix .",
    "sonar": "node sonar-project.js",
    "start": "node ./bin/www",
    "server": "babel-node ./src/bin/www",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sonarqube-scanner": "^2.5.0"
  }
}
